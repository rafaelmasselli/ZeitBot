{
  "name": "zeitbot-clean-arch",
  "version": "1.0.0",
  "description": "ZeitBot com Clean Architecture",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:ollama": "ts-node -r tsconfig-paths/register src/behavior/test-ollama-real.ts",
    "test:ai": "ts-node -r tsconfig-paths/register src/behavior/test-ai-recommendations.ts",
    "test:embeddings": "ts-node -r tsconfig-paths/register src/behavior/test-embeddings.ts",
    "test:news": "ts-node -r tsconfig-paths/register src/behavior/test-news-collection.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@typegoose/typegoose": "^12.17.0",
    "@types/cheerio": "^1.0.0",
    "@types/cron": "^2.4.3",
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.1",
    "mongoose": "^8.16.1",
    "node-cron": "^3.0.3",
    "qrcode-terminal": "^0.12.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "whatsapp-web.js": "^1.31.0",
    "winston": "^3.13.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/xml2js": "^0.4.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
